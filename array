#include <stdio.h>
#define m 1000000
int main() {
	// your code goes here
	int T;
	scanf("%d\n",&T);
	for(int i=0;i<T;i++){
	    int N,a[m],max=0;
	    scanf("%d",&N);
	    a[0]=max;
	    for(int j=0;j<N;j++){
	        scanf("%d",&a[j]);
	        if(a[j]>max)
	            max=a[j];
	    }
	    printf("%d\n",max);
	    
    }
}

